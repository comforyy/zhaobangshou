<view class='bg_select' style="width:100%;height:35px">
    <text>需求个人信息</text>
  </view>
<view class="form">
<view class="input-group">
    <label>👫  称呼：</label>
    <text >{{pick.identification.familyName}}</text>
  </view>
  <view class="input-group" data-callPhone='{{pick.phone}}' bindtap='callPhone'>
    <label>☎️  电话</label>
    <text >{{pick.phone}}</text>
  </view>
  <view class="input-group">
    <label>🌏  地址</label>
    <text>{{pick.address}}</text>
  </view>
</view>
<button style="margin-left: 15rpx; margin-right: 15rpx; margin-top: 100rpx; border-radius: 40rpx;background: #5bb9e9" type="primary" bindtap="pucancel">返回</button>