.bg_select {
  background-color: #f1f1f1;
  display: flex;
  justify-content: center;
  flex-direction: column;
  width: 100%;
  height: 80rpx;
  font-size: 26rpx;
  padding-left: 10rpx;
}

.contion {
  padding-top: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.progress_box {
  position: relative;
  width: 220px;
  height: 220px;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #fff;
}

.progress_bg {
  position: absolute;
  width: 220px;
  height: 220px;
}

.progress_canvas {
  width: 220px;
  height: 220px;
}

.progress_text {
  position: absolute;
  display: flex;
  align-items: center;
  justify-content: center;
}

.progress_info {
  font-size: 36rpx;
  padding-left: 16rpx;
  letter-spacing: 2rpx;
}

.progress_dot {
  width: 16rpx;
  height: 16rpx;
  border-radius: 50%;
  background-color: #fb9126;
}

.oder_bg {
  padding-top: 80rpx;
  height: 220rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}

.oder_tx {
  width: 80%;
  font-size: 30rpx;
}

.line {
  margin-top: 20rpx;
  width: 80%;
  height: 2rpx;
  background: #aaa;
}

.contaion {
  display: flex;
  flex-direction: column;
  padding-left: 20rpx;
  margin-top: 20rpx;
}

.pichup_bg {
  height: 200rpx;
  width: 95%;
  background-color: #fff;
  box-shadow: #888 2.5px 2.5px 7.5px 1.25px;
  padding-left: 20rpx;
  margin-top: 20rpx;
}

.EnMessage {
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 20rpx;
}

.EnIcon {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
}

.EnName {
  padding-left: 20rpx;
  font-size: 24rpx;
}

.Enlocal {
  padding-left: 20rpx;
  font-size: 24rpx;
}

.Message {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.EnPhone {
  margin: 20rpx;
  font-size: 24rpx;
}

.price{
  font-size: 30rpx;
  color: red;
}

.button {
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.btn {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  background-color: #59df18;
  margin-top: 100rpx;
  padding: 10px 20px;
  border-radius: 5px;
  width: 100px;
  height: 20px;
  color: white;
}

.btn-1 {
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  background-color: #fec01c;
  margin-top: 100rpx;
  padding: 10px 20px;
  border-radius: 5px;
  width: 100px;
  height: 20px;
  margin-left: 10rpx;
  color: white;
}

.btn-3 {
  position: fixed;
  bottom: 0;
  width: 100%;
  margin-top: 30rpx;
  background-color: #5bb9e9;
}
